{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "quotes":"off",
        "no-console": "off",
        "no-else-return": "off",
        "no-await-in-loop": "off",
        "no-restricted-syntax": "off",
        "linebreak-style": "off",
        "no-plusplus": "off",
        "max-len": "off",
        "no-use-before-define":"warn",
        "no-return-await": "off",
        "camelcase": "off",
        "indent":"off",
        "no-unused-vars": "warn"
    },
    "ignorePatterns":["./src"]
}
